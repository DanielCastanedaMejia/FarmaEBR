<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:l="sap.ui.layout"
   xmlns:core="sap.ui.core" >
  <Dialog
     id="stopOrderDialog"
     title="FINALIZAR ORDEN">
    <l:VerticalLayout
      class="sapUiContentPadding"
      width="100%">
      <l:content>
        <!-- Filter by date range -->
        <Label text="Orden"/>
        <Input
        id="inputOrder1"
        class="sapUiSmallMarginBottom" />
      </l:content>
    </l:VerticalLayout>

    <beginButton>
      <Button
       type="Accept"
         text="Aceptar"
         icon="sap-icon://add-product"
         press="onChangeStatusStop"/>
    </beginButton>
    <endButton>
      <Button
         type="Reject"
         text="Cancelar"
         icon="sap-icon://cancel"
         press="onCloseDialogStopOrder"/>
    </endButton>

  </Dialog>
</core:FragmentDefinition>