<core:FragmentDefinition id="spotEditFragment"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:u="sap.ui.unified"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core" class="sapUiSizeCompact">
    <Dialog title="Modificar spot" id="editSpotDialog">
        <beginButton>
            <Button type="Accept" text="Guardar cambios" press="onSaveSpot"/>
        </beginButton>
        <endButton>
            <Button id="editSpot" type="Reject" text="Cancelar" press="onCloseDialog"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition> 