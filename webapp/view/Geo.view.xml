<mvc:View
    controllerName="sap.ui.demo.webapp.controller.Geo"
    xmlns:uxap="sap.uxap"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:layout="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
    xmlns:vbm="sap.ui.vbm" 
    xmlns:mc="sap.suite.ui.microchart" 
    xmlns:ce="sap.ui.codeeditor" 
	xmlns:mvc="sap.ui.core.mvc">
  <Page id="map">

    <customHeader>
        <Toolbar
          class="toolbar_default">
          <Button icon="sap-icon://nav-back" type="Transparent" class="sapUiMediumMarginBegin" press="onNavBack"></Button>
          <Image src="images/SAP_Logo.png">
          </Image>
          <core:Icon
          src="sap-icon://map-2"
          class="toolbar_title sapUiSmallMarginBegin">
            <core:layoutData>
              <FlexItemData growFactor="1" />
            </core:layoutData>
          </core:Icon>
          <MenuButton text="Geolocalización" class="customtitle">
            <menu>
              <Menu>
                <items>
                  <MenuItem text="Página Inicial" icon="sap-icon://home" press="goToHome"/>
                </items>
              </Menu>
            </menu>
          </MenuButton>
          <ToolbarSpacer/>
          <Label
                   id ="username"
                   text="{masterModel>/view/login/username}"
                   class="toolbar_color"></Label>
          <Button icon="sap-icon://person-placeholder" type="Transparent" class="sapUiLargeMarginEnd" enabled="false">
          </Button>
        </Toolbar>
      </customHeader>
      <content>
        
      </content>
  </Page>
</mvc:View>