<core:FragmentDefinition
    controllerName="sap.ui.demo.webapp.controller.PMReport.viewPMReporteTiempo"
    xmlns:form="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:viz.data="sap.viz.ui5.data">
  <Dialog
     id="PMGraphicActivities"
     title="ORDENES REALIZADAS">
    <l:VerticalLayout
      class="sapUiContentPadding"
      width="100%">
      <l:content>

                    <viz:Popover id="idPopOver"></viz:Popover>
                    <viz:VizFrame xmlns="sap.viz" id="idcolumn" width="100%" uiConfig="{applicationSet:'fiori'}">
                    </viz:VizFrame>

      </l:content>
    </l:VerticalLayout>
    <beginButton>
      <Button
       type="Accept"
         text="{i18n>Accept}"
         press="onCloseDialogCreateUser"/>
    </beginButton>
    <endButton>
      <Button
         type="Reject"
         text="{i18n>LabelCancel}"
         press="onCloseDialogNewUser"/>
    </endButton>

  </Dialog>
</core:FragmentDefinition>